  "components": {
    "schemas": {
      "BookState": {
        "type": "integer"
      },
      "BookTimeSlotRequest": {
        "required": [
          "plannedDayId",
          "courtId",
          "timeSlotNumber",
          "weekNumber",
          "bookState"
        ],
        "type": "object",
        "properties": {
          "plannedDayId": {
            "type": "string",
            "format": "uuid"
          },
          "courtId": {
            "type": "string",
            "format": "uuid"
          },
          "timeSlotNumber": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          },
          "weekNumber": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          },
          "bookState": {
            "$ref": "#/components/schemas/BookState"
          }
        }
      },
      "Comparison": {
        "type": "integer"
      },
      "CourtResponse": {
        "required": [
          "id",
          "number"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "number": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          }
        }
      },
      "CreateCourtRequest": {
        "required": [
          "number"
        ],
        "type": "object",
        "properties": {
          "number": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          }
        }
      },
      "CreatePlannedDayRequest": {
        "required": [
          "dayOfWeek",
          "numberOfTimeSlots"
        ],
        "type": "object",
        "properties": {
          "dayOfWeek": {
            "$ref": "#/components/schemas/DayOfWeek"
          },
          "numberOfTimeSlots": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          }
        }
      },
      "CreateSiteRequest": {
        "required": [
          "name",
          "closedDays",
          "courts",
          "schedule"
        ],
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "closedDays": {
            "type": [
              "null",
              "array"
            ],
            "items": {
              "type": "string",
              "format": "date"
            }
          },
          "courts": {
            "type": [
              "null",
              "array"
            ],
            "items": {
              "$ref": "#/components/schemas/CreateCourtRequest"
            }
          },
          "schedule": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CreatePlannedDayRequest"
            }
          }
        }
      },
      "DayOfWeek": {
        "type": "integer"
      },
      "ExpressionFilter": {
        "type": "object",
        "properties": {
          "propertyName": {
            "type": [
              "null",
              "string"
            ]
          },
          "valueString": {
            "type": [
              "null",
              "string"
            ]
          },
          "comparison": {
            "$ref": "#/components/schemas/Comparison"
          },
          "filterAssociation": {
            "$ref": "#/components/schemas/FilterAssociation"
          }
        }
      },
      "FilterAssociation": {
        "type": "integer"
      },
      "FilterGroup": {
        "type": "object",
        "properties": {
          "filters": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ExpressionFilter"
